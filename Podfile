source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '12.1'
inhibit_all_warnings!

target 'JobTracker' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for JobTracker
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'
end
